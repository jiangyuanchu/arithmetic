<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jyc.graduation.individuation.mapper.UserHistoryMapper" >

  <insert id="insert" parameterType="com.jyc.graduation.individuation.domain.UserHistory">
    insert into user_history (user_id ,word ,properties ,`type`)
    values (#{userId} ,#{word} ,#{properties} ,#{type})
  </insert>

</mapper>